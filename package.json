{
  "name": "takkr",
  "module": "index.tsx",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "./bin/dev.sh",
    "start": "NODE_ENV=production bun index.tsx",
    "build": "bun run build:css && bun run check",
    "build:css": "vite build",
    "build:docs": "bun bin/export-openapi.ts",
    "test": "bun test",
    "check": "tsc --noEmit",
    "lint": "biome check --write .",
    "clean": "rm -rf www/styles.css www/entry.js www/openapi.json",
    "css": "vite build --watch",
    "caddy:stop": "caddy stop"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.12",
    "@tailwindcss/vite": "^4.1.10",
    "@types/bun": "latest",
    "tailwindcss": "^4.1.10",
    "vite": "^6.3.5"
  },
  "peerDependencies": {
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@hono/zod-openapi": "^1.2.2",
    "@scalar/hono-api-reference": "^0.9.45",
    "@simplewebauthn/browser": "^13.2.2",
    "@simplewebauthn/server": "^13.2.2",
    "alpinejs": "^3.15.4",
    "hono": "^4.11.5",
    "htmx-ext-sse": "^2.2.4",
    "htmx.org": "^2.0.8",
    "scalar": "^0.3.4",
    "surrge": "^0.11.5",
    "zod": "^4.3.6"
  }
}
